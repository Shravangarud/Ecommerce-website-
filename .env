# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection String
# Replace <username>, <password>, and <cluster-url> with your MongoDB credentials
MONGO_URI=mongodb://localhost:27017/ecommerce
# For MongoDB Atlas, use: mongodb+srv://<username>:<password>@<cluster-url>/ecommerce?retryWrites=true&w=majority

# JWT Secret Key (Change this to a random string in production)
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_EXPIRE=30d
